package TwoClasses;

public class Student {
	public String name = "no name"; // our student class has 3 properties
	public int rollno = 0 ;
	public int age = 0;
	public boolean scholarship = false;
	public String level = "undefined";
	 
	public void introduce(){ //create a method is not going to take any valuer or return any and access the properties of this class
	  System.out.println("my name is "+ name);
	  System.out.println("my rollno is "+ rollno);
	  System.out.println("my age is "+ age);
	  System.out.println("scholarship? "+ scholarship);
	  System.out.println("Level? "+ level);
	  System.out.println();
	 }
	 
 
	/* boolean hasScholarship(){ //method here for the scholarship. Method type and method name
